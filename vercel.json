{
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "regions": ["iad1"],
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "https://boomerang-connect-web.vercel.app/:path*",
      "destination": "https://boomerangconnect.vercel.app/:path*",
      "permanent": true
    }
  ]
}
